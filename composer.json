{
	"name": "duon/quma",
	"description": "A no-ORM database library for executing raw SQL files",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "ebene fÃ¼nf GmbH",
			"email": "duon@ebenefuenf.de",
			"role": "Developer"
		}
	],
	"homepage": "https://duon.dev/quma",
	"keywords": [
		"duon",
		"database",
		"sql"
	],
	"autoload": {
		"psr-4": {
			"Duon\\Quma\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Duon\\Quma\\Tests\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=8.1",
		"duon/cli": "dev-main"
	},
	"require-dev": {
		"duon/dev": "dev-main",
		"phpunit/phpunit": "^11.5",
		"vimeo/psalm": "^6.10",
		"dealerdirect/phpcodesniffer-composer-installer": "^1",
		"staabm/annotate-pull-request-from-checkstyle": "^1"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
